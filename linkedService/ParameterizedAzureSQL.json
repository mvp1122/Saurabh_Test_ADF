{
	"name": "ParameterizedAzureSQL",
	"type": "Microsoft.DataFactory/factories/linkedservices",
	"properties": {
		"parameters": {
			"AzureSQLDBName": {
				"type": "string"
			},
			"AzureSQLDBPassword": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "AzureSqlDatabase",
		"typeProperties": {
			"connectionString": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=azuresql-mvp1122.database.windows.net;Initial Catalog=@{linkedService().AzureSQLDBName};User ID=Azure_sql_server;Password=@{linkedService().AzureSQLDBPassword}"
		}
	}
}